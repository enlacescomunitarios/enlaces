$palette = ({'base_color':#8abe9b, 'back_color':#5c8692, 'active_color':#4a6d8b}) ({'base_color':#0d58a2, 'back_color':#365879, 'active_color':#d87a25}) ({'base_color':#14a085, 'back_color':#4a8b8b, 'active_color':#0d455d}) ({'base_color':#d40000, 'back_color':#333333, 'active_color':#800000})
$choice = 0//0, 1, 2 or 3
base_color = $palette[$choice]['base_color']
back_color = $palette[$choice]['back_color']
active_color = $palette[$choice]['active_color']
@import 'functions'
*
	text-rendering optimizeLegibility !important
	-webkit-font-smoothing antialiased !important
	-moz-osx-font-smoothing grayscale !important
	outline none !important
html, body
	width 100% !important
	height 100% !important
	margin 0
	padding 0
body
	//background-image radial-gradient(to bottom right, back_color, rgba(255,255,255,.5))
	//background-image radial-gradient(ellipse at bottom right, rgba(230,239,248,.9) 0%, back_color 100%)
	//background radial-gradient(ellipse at bottom right, rgba(210,224,235,1) 0%, rgba(57,108,123,1) 100%)
	background radial-gradient(ellipse at bottom right, lighten(saturate(lighten(back_color,78%),24.5%),10%) 0%, darken(saturate(back_color, 39%),23%) 100%)
	background-repeat no-repeat
	background-attachment fixed
	margin-top 45px !important
	#breadcrumbs
		margin-top 60px !important
		margin-bottom 0 !important
		border none
		border-radius 0px
		background-color transparent !important
		fix_outborder()
		& *, &
			color #fff !important
			li.active
				color darken(#fff, 15%) !important
		&>li+li:before
			//content "\007C"
			padding 0 2px !important
			color darken(active_color, 15%) !important
			font-size 10px !important
	#container
		//background-color #f9f9f9 !important
		margin 15px auto !important
		padding 0 !important
		background-color transparent !important
		position absolute
		width 100%
		#left-content, #right-content
			//background-color #fff !important
			background-color transparent !important
			fix_outborder()
		#left-content
			padding 15px !important
			.fix-table
				//margin 15px !important
				margin-bottom 15px !important
				padding 15px
				border-radius 5px
				//margin-bottom 15px !important
				background-color #fff !important
				fix_outborder()
				&:last-child
					margin-bottom 0 !important
			.detail-card + .btn-actions, .rowBox + .fix-table, .btn-actions + .fix-table, .detail-card + .fix-table, .fix-table + .btn-actions
				margin-top 15px !important
			.detail-card
				.dl-horizontal
					margin-bottom 0 !important
				//margin 15px !important
				padding 15px !important
				//padding-bottom 0 !important
				background-color #fff !important
				fix_outborder(1)
				& *
					font-size 12px !important
			//.btn-actions
				//margin 15px !important
			form
				margin 0 auto
				padding 15px !important
				//padding 0 !important
				legend
					margin-bottom 15px !important
				textarea
					width 99.8% !important
				.form-group:last-child
					margin-bottom 0 !important
				p.help-block
					margin-bottom 0 !important
				[class*="col-"]
					fix_position()
				.row
					fix_position(1,14,14)
					margin-left -16px !important
					margin-right -16px !important
					//margin-bottom 15px !important
					[class*="col-"]
						margin 0 !important
				// .custom-row
				// 	margin 0
				// 	padding 0
				// 	padding-bottom 15px
				// 	width 360px !important
				// 	.row
				// 		margin-left -15px
				// 		width 360px !important
				// 		.form-control-feedback
				// 			right 2px
				// 	label
				// 		margin-left 0
				// 		width 360px !important
				&:not(.big-form)
					background-color #fff !important
					min-width 280px !important
					width 360px !important
					//padding 15px !important
					fix_outborder()
		#right-content
			padding 15px !important
			.list-group
				background-clip border-box !important
				.list-group-item
					border 1px solid lighten(active_color, 35%) !important
					&:first-child
						border-top-left-radius 5px !important
						border-top-right-radius 5px !important
					&:last-child
						border-bottom-left-radius 5px !important
						border-bottom-right-radius 5px !important
[class*="only_"]:not([class*="_login"]):not([class*="_passwd"]):not([class*="_lgpwd"])
	text-transform uppercase
.navbar-inverse
	background-color base_color !important
	.navbar-nav
		&>li>a, &>.active>a
			&:hover, &:focus
				background-color darken(active_color, 15%) !important
	.navbar-toggle
		background-color base_color !important
		border 1px solid #fff
	.navbar-collapse
		border-color transparent
	.dropdown-menu
		background-color base_color
		& a:hover
			background-color darken(active_color, 15%) !important
.nav
	li:hover
		background-color back_color
#right-content .list-group-item.active,
.navbar-inverse .navbar-nav>.open>a,
.navbar-inverse .navbar-nav>.active>a,
.navbar-inverse .navbar-nav .open .dropdown-menu>.active>a
	hover_normal()
	&:hover, &:focus
		hover_active()
@media (max-width: 380px)
	body
		&>#container
			form
				width 85%
				legend
					font-size 17px
@media (min-width: 768px)
	body
		#container
			#left-content
				.detail-card
					.dl-horizontal
						dt
							//width auto !important
							width 30% !important
						dd
							//margin-left 20% !important
							margin-left 31% !important
// Big form by Limber Mancilla
$padding-bottom = 3px
@media (max-width: 340px)
	fix_form_fields()
	body
		#container
			#left-content, #right-content
				margin 0 !important
				padding 0 !important
			#right-content
				margin-top 15px !important
	#left-content
		form
			background-color #fff !important
			margin 0 auto !important
			//padding 15px !important
			width 100% !important
@media (min-width: 341px) and (max-width: 639px)
	fix_form_fields()
	body
		#container
			#left-content, #right-content
				margin 0 !important
				padding 0 !important
			#right-content
				margin-top 15px !important
	#left-content
		padding 0 !important
		form
			//margin 15px auto !important
			padding 0 !important
			width 320px !important
			.inner-container
				padding 0 !important
				margin-bottom 15px !important
				.left-side, .right-side
					padding 15px !important
					margin-bottom 0 !important
					padding-bottom $padding-bottom !important
					background-color #fff !important
					fix_outborder()
					display block
					vertical-align top
				.left-side
					border-bottom-left-radius 0 !important
					border-bottom-right-radius 0 !important
				.right-side
					border-top-left-radius 0 !important
					border-top-right-radius 0 !important
@media (min-width: 640px)
	fix_form_fields()
	#left-content
		form
			//margin 15px auto !important
			padding 0 !important
			&:not(.big-form)
				width 320px !important
		form.big-form
			margin 0 !important
			padding 0 !important
			.inner-container + .form-group
				margin-top 15px !important
			.inner-container
				width 100% !important
				margin 0 !important
				padding 0 !important
				.left-side, .right-side
					background-color #fff !important
					margin 0 !important
					padding 15px !important
					margin-bottom 0 !important
					padding-bottom $padding-bottom !important
					width 49.5% !important
					display inline-block !important
					vertical-align top !important
					fix_outborder()
				.left-side
					top 0 !important
					left 0 !important
					position relative !important
				.right-side
					top 0 !important
					right -5px !important
					position relative !important
// End Big form
@media (max-width: 931px)
	body
		#breadcrumbs
			margin-left 0 !important
			margin-right 0 !important
		#container
			#left-content
				margin-bottom 15px
				float none
			#right-content
				margin-top 15px
				float none
				z-index -1
				.list-group
					text-align center
					margin-bottom 0
			&:after
				fix_after_container()
@media (min-width: 932px)
	body
		#breadcrumbs
			margin-left 15px !important
			margin-right 15px !important
		#container
			#left-content
				float left !important
				margin-left 15px
				width 75.9% !important
				margin-bottom 15px
			#right-content
				float right !important
				margin-right 15px !important
				top 0 !important
				right 0 !important
				position absolute !important
				width 20% !important
				.list-group
					text-align right
					margin-bottom 0
			&:after
				fix_after_container(25)
#footer
	width 100%
	bottom 0 !important
	position fixed
	z-index 3
	*
		border-collapse 0
		border-spacing 0
		margin 0 auto
		padding 0
		text-align center
		vertical-align bottom
	&:before
		content " "
		display block
	&>.foot-container
		padding-top 2px
		width 100%
		height 25px
		background-color base_color
		color #fff
		overflow hidden
	.text-muted
		color #fff
select.form-control
	padding-left 2px !important
	width 99.8% !important
.form-control
	padding-left 5px !important
	padding-right 2px !important
.has-feedback
	.form-control
		padding-right 0 !important
	&~.form-control-feedback
		right 0 !important
.link, a
	cursor pointer
	color darken(#5bc0de, 15%)//color #088DA5
	&:hover, &:focus
		color darken(#5bc0de, 30%)//color #0697ff
		text-decoration underline
.input-group-addon
	padding 8px 10px
	font-size 14px
// Begin dataTables
.dataTables_filter
	input[type="search"]
		height 26px !important
		padding 4px 8px !important
.dataTables_scrollHeadInner
	tr
		&:first-child
			font-size 16px
			background-color saturate(base_color,25%)
			fix_colorText()
			th
				border none !important
		&:last-child
			font-size 12px
			background-color saturate(active_color,25%)
			fix_colorText()
			th
				border-top none !important
				border-bottom none !important
//.pagination
	//&>.disabled
		//&>span, &>a
			//fix_pagination()
			//&:hover, &:focus
				//fix_pagination()
[id$="_wrapper"]
	.row:nth-child(2)
		tbody
			tr
				td
					height 31.818px !important
	.row:nth-child(3)
		background-color darken(#fff, 15%) !important
		margin-top 15px !important
		margin-bottom -15px !important
		padding 0 !important
		border-bottom-left-radius 5px !important
		border-bottom-right-radius 5px !important
		[class*="col-sm-"]
			//display inline-block !important
			//text-align center !important
			//background-color darken(#fff, 15%) !important
			background-color transparent !important
			display flex !important
			&:first-child
				border-bottom-left-radius 5px !important
				div.dataTables_paginate
					text-align left !important
					ul
						vertical-align top !important
						& *
							border-collapse collapse
							border-spacing -1px !important
							margin 0 !important
						[id$="_first"], [id$="_previous"], [id$="_next"], [id$="_last"]
							a
								font-family: FontAwesome;
								display: inline;
						[id$="_first"] a
							&:before
								content "\f048"
						[id$="_previous"] a
							&:before
								content "\f04a"//"\f053"
								margin-left -5px !important
						[id$="_next"] a
							&:before
								content "\f04e"//"\f054"
								margin-right -5px !important
						[id$="_last"] a
							&:before
								content "\f051"
			&:nth-child(2)
				white-space nowrap
				vertical-align top !important
				padding-bottom 1px !important
				//padding-right 0 !important
				label
					margin 0 !important
					padding 0 !important
					top -1px !important
					position relative !important
				select
					padding 0 !important
					width 55px !important
					height 18px !important
			&:nth-child(3)
				//padding-left 0 !important
				//padding-right 0 !important
				border-bottom-right-radius 5px !important
				justify-content flex-end !important
				.dataTables_info
					padding 3px 0 !important
					white-space nowrap
					vertical-align top !important
					font-size 12px !important
					//float right !important
@media (-webkit-min-device-pixel-ratio:0)
	[id$="_wrapper"]
		.row:nth-child(3)
			[class*="col-sm-"]
				&:nth-child(2)
					padding-bottom 0 !important
					select
						top -1px !important
						position relative !important
@media (max-width: 767px)
	[id$="_wrapper"]
		.row:nth-child(3)
			margin-bottom -15px !important
			[class*="col-sm-"]
				display inline-block !important
				&:first-child
					width 100% !important
				&:nth-child(2)
					padding-top 0 !important
					top 1px !important
					position relative !important
				&:nth-child(3)
					.dataTables_info
						padding-bottom 0 !important
@media (max-width: 767px) and (-webkit-min-device-pixel-ratio:0)
	[id$="_wrapper"]
		.row:nth-child(3)
			[class*="col-sm-"]
				&:nth-child(2)
					padding-top 0 !important
					top 2px !important
					position relative !important
				&:nth-child(3)
					padding 2px 15px !important
