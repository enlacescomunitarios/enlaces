// End dataTables
.rowBox
	width 100% !important
	text-align center
	display flex
	flex-direction row
	.inner-box
		fixBoxCard()
		height @width !important
		color #fff
		text-shadow darken(@color,45%) 0px 0px 1px
		padding 15px !important
		border-radius 5px !important
		.number
			vertical-align top
			font-size 30px
		p
			bottom -15px !important
			position relative
			font-size 14px
		&:first-child
			background-color #1f8dd6
			margin-right 15px
			makeBorder()
			.badge
				fixBadge()
			&:hover
				hoverBoxCard()
		&:nth-child(2)
			background-color darken(rgb(250, 210, 50),10%)
			margin-right 15px
			makeBorder()
			.badge
				fixBadge()
			&:hover
				hoverBoxCard()
		&:nth-child(3)
			background-color darken(#5eb95e,10%)
			margin-right 15px
			makeBorder()
			.badge
				fixBadge()
			&:hover
				hoverBoxCard()
		&:last-child
			background-color rgb(243, 123, 29)
			makeBorder()
			.badge
				fixBadge()
			&:hover
				hoverBoxCard()
@media (max-width:389px)
	.rowBox
		//width 100% !important
		//display flex
		flex-direction column !important
		.inner-box
			width 100% !important
@media (min-width:390px) and (max-width: 639px)
	.rowBox
		//width 100% !important
		//display flex
		//flex-direction row
		flex-wrap wrap
		.inner-box
			&:first-child
				margin-bottom 15px
			&:nth-child(2)
				margin-right 0 !important
				margin-bottom 15px
				right 0px
				position absolute
			&:nth-child(4)
				margin-right 0 !important
				right 0px
				position absolute
			fixBoxCard(100%, 2, 1px)
.tooltip
	&.bottom
		margin-top -1px !important
	$color = active_color//#f04124
	$opacity = .8
	.tooltip-arrow
		border-bottom-color $color !important
		opacity $opacity
	.tooltip-inner
		background-color $color !important
		font-size 11px !important
		text-shadow darken($color,65%) 0px 0px 1px
		opacity $opacity